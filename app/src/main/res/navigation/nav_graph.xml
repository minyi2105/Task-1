<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/movieMainActivity">
    <activity
        android:id="@+id/movieMainActivity"
        android:name="com.example.fourtitudetask1.activities.MovieMainActivity"
        android:label="activity_movie_main"
        tools:layout="@layout/activity_movie_main" >
        <argument android:name="imdbId"
            app:argType="string" />
    </activity>
    <activity
        android:id="@+id/movieDetailActivity"
        android:name="com.example.fourtitudetask1.activities.MovieDetailActivity"
        android:label="activity_movie_detail"
        tools:layout="@layout/activity_movie_detail" />
</navigation>